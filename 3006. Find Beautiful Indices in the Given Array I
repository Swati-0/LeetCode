class Solution {
public:
    vector<int> beautifulIndices(string s, string a, string b, int k) {
        vector<int>ans;
        int n=s.length();
        int x=a.length();
        int y=b.length();
        if(n<x || n<y)
        return {};
        vector<int>v1,v2;
        for(int i=0; i<=n-x; i++){
            if(s.substr(i,x)==a){
                v1.push_back(i);
                cout<<i<<endl;
            }
        }
        for(int i=0; i<=n-y; i++){
            if(s.substr(i,y)==b){
                v2.push_back(i);
                cout<<i<<endl;
            }
        }
        for(int i=0; i<v1.size(); i++){
            for(int j=0; j<v2.size(); j++){
                if(abs(v1[i]-v2[j])<=k){
                    ans.push_back(v1[i]);
                    break;
                }
            }
        }
        return ans;
    }
};
