class Solution {
public:
    int maxDepth(string s) {
        int maxi=0;
        int cnt=0;
        int n=s.length();
        for(int i=0; i<n; i++){
            if(s[i]=='('){
                cnt++;
                maxi=max(maxi,cnt);
            }else if(s[i]==')'){
                cnt--;
                maxi=max(maxi,cnt);
            }
        }
        return maxi;
    }
};
