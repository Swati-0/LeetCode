class Solution {
public:
    int beautySum(string s) {
        int l=s.length();
        int sum=0;
        for(int i=0; i<l; i++){
            unordered_map<char,int>mp;
            for(int j=i; j<l; j++){
                mp[s[j]]++;
                int mini=INT_MAX,maxi=0;
                for(auto it:mp){
                    maxi=max(maxi,it.second);
                    mini=min(mini, it.second);
                }
                sum+=(maxi-mini);
            }
        }
        return sum;
    }
};
