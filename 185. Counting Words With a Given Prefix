class Solution {
public:
    int prefixCount(vector<string>& words, string pref) {
        int n=words.size();
        int p=pref.size();
        int cnt=0;
        for(int i=0; i<n; i++){
            if(p<=words[i].size()){
                if(words[i].substr(0,p)==pref)
                cnt++;
            }
        }
        return cnt;
    }
};
